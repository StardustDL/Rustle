<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.0.1-alpha</Version>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>StardustDL</Authors>
    <Description>The core types of Rustle.</Description>
    <Copyright>Copyright (c) StardustDL. All rights reserved.</Copyright>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/StardustDL/Rustle</PackageProjectUrl>
    <RepositoryUrl>https://github.com/StardustDL/Rustle</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Rust</PackageTags>
  </PropertyGroup>

</Project>
